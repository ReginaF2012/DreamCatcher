<div class="container">
  <br>
  <% if @symbolisms.empty? %>
    <h3></h3>
  <% else %>
    <% if @user && @user == current_user %>
      <%= render 'users_symbolisms' %>
    <% elsif @user %>
      <%= render 'users_public_symbolisms' %>
    <% else%>
      <%= render 'public_symbolisms' %>
    <% end%>
  <% end %>
  <br>
</div>