<div class="container">
  <h1>Login with Google</h1>
  <%= link_to(image_tag("/images/btn_google_signin_dark_normal_web.png"), "/auth/google_oauth2") %>
  <h1>Login with Email</h1>
  <%=  form_with url: "/login", method: :post do |f|%>
    <div class="form-group">
      <%= f.label "Email:" %><br>
      <%= f.email_field :email %>
    </div>
    <div class="form-group">
      <%= f.label "Password:" %><br>
      <%= f.password_field :password %>
    </div>
    <%= f.submit "Log In" %>
  <% end %>
</div>