<div class="container">
  <h1><%= link_to "Log In with Google", "/auth/google_oauth2" %></h1>
  <h1>Log In with Email</h1>
  <%=  form_with url: "/login", method: :post do |f|%>
    <div class="form-group">
      <%= f.label :email, "Email:" %><br>
      <%= f.email_field :email %>
    </div>
    <div class="form-group">
      <%= f.label :password, "Password:" %><br>
      <%= f.password_field :password %>
    </div>
    <%= f.submit "Log In" %>
  <% end %>
</div>